@model SearchViewModel
@{
    ViewData["Title"] = "GetData";
}

<h1>Search results</h1>


<div class="row">
    <div class="col-md-2">
        <h3>Projects</h3>
        <ul class="list-group">
            @foreach (var item in Model.Projects)
            {
                <li class="list-group-item">
                    @item.Name
                </li>
            }
        </ul>
    </div>
    <div class="col-md-2">
        <h3>Tasks</h3>
    </div>
    <div class="col-md-2">
        <h3>Activities</h3>
    </div>
    <div class="col-md-2">
        <h3>Users</h3>
    </div>
    <div class="col-md-2">
        <h3>Templates</h3>
    </div>

</div>



